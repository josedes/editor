<div class="row">
	<div class="col-md-3 center-block no-float">

<h3><i class="fa fa-user-plus"></i> Registrese</h3>


<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

<div>  <div class="form-group"><%= f.label :nombre %><br />
  <%= f.text_field :nombre, autofocus: true, class: "form-control" %></div>

<div>  <div class="form-group"><%= f.label :apellido %><br />
  <%= f.text_field :apellido, autofocus: true, class: "form-control" %></div>

  <div>  <div class="form-group"><%= f.label :username %><br />
  <%= f.text_field :username, autofocus: true, class: "form-control" %></div>

<div>  <div class="form-group"><%= f.label :email %><br />
  <%= f.email_field :email, autofocus: true, class: "form-control" %></div>

<div class="form-group"><%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", class: "form-control" %></div>

<div class="form-group"><%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control" %></div>

 <div class="text-center"><%= f.submit "Registrese", class: "btn btn-info" %></div>
<% end %>

<div class="text-center">
<%= render "devise/shared/links" %>
</div>




</div>
</div>
